# Minimal production example (Render)
# Kopiera till .env och ersätt PLACEHOLDERS. INGA riktiga hemligheter här.
NODE_ENV=production
SERVER_URL=https://api.example.com           # Byt till din riktiga API-domän
DISABLE_NEXT_APP=true
DEFAULT_LOCALE=en
FILE_FIELD_ADAPTER=local

# Postgres (extern URL med sslmode)
DATABASE_URL=postgresql://USER:PASSWORD@HOST/DBNAME?sslmode=require

# Redis / Valkey (TLS)
REDIS_URL=rediss://INSTANCE:AUTH_PASSWORD@HOST:6379

# 64+ tecken slumpad hemlighet
COOKIE_SECRET=CHANGE_ME_GENERATE_64_CHARS

# Optional tuning
DISABLE_RATE_LIMIT=true
DISABLE_LOGGING=false
WORKER_CONCURRENCY=50
USE_LOCAL_FEATURE_FLAGS=true
DISABLE_CAPTCHA=true
ADAPTER_CACHE_CONFIG='{"enabled":true,"maxCacheSize":1000}'
REQUEST_CACHE_CONFIG='{"enabled":true}'

# Email/help metadata
HELP_REQUISITES='{ "support_email": "help@example.com", "support_email_mobile": "helpmobile@example.com", "bot_email": "service@example.com", "support_phone": "+1 301 000-00-00" }'

# Mobile app ids (exempel)
MOBILE_APP_IDS='{ "resident": { "ios": "ai.doma.clients", "android": "ai.doma.client" }, "master": { "ios": "ai.doma.master", "android": "ai.doma.master" }}'
TELEGRAM_EMPLOYEE_BOT_TOKEN=telegram-token

# AI (deaktivera om ej används)
AI_ENABLED=false
AI_FLOWS_CONFIG='{}'
AI_ADAPTERS_CONFIG='{}'

# Placeholder domäner (lokal dev exempel) – ta bort om ej behövs
ADDRESS_SERVICE_DOMAIN=http://localhost:4001
ANNOUNCEMENTS_DOMAIN=http://localhost:4002
ASTERISK_BOT_DOMAIN=http://localhost:4003
# ... lägg till fler vid behov

# För avancerade feature toggles / OIDC se originalrepo dokumentation
#OIDC_CONDO_CLIENT_CONFIG='{"serverUrl":"https://frontend.example.com","clientId":"<CLIENT_ID>","clientSecret":"<CLIENT_SECRET>"}'
# FEATURE_TOGGLE_CONFIG='{"url":"https://features.example.com/api/features","apiKey":"key"}'
